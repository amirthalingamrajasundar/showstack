{
    "version": 2,
    "builds": [
        {
            "src": "build/index.html",
            "use": "@vercel/static"
        },
        {
            "src": "build/**",
            "use": "@vercel/static"
        }
    ],
    "rewrites": [
        {
        "source": "/:match*",
        "destination": "/index.html"
        }
    ]
}
